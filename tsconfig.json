{
  "compilerOptions": {
    "baseUrl": "src",
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES2019"],
    "module": "CommonJS",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "target": "ES2019",
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "exclude": ["node_modules", "out", ".vscode-test"]
}
